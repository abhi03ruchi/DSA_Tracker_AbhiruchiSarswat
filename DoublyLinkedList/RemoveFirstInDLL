class Node{
    int data;
    Node prev;
    Node next;
    Node(int x){
        data = x;
        prev = null;
        next = null; 
  }
}
public class deleteBeginDLL {
    public static Node deleteHead(Node head){
        if(head == null  || head.next == null){
            return null;
        }
        head = head.next;
        head.prev = null;
        return head;
    }
    }
