public class genz {
    public static void printD(int n){
        if(n == 0){
            return;
        }
        System.out.println(n);
        printD(n-1);
    }
    public static void printI(int n){
        if(n == 0){
            return;
        }
        printI(n-1);
        System.out.println(n);
    }
    public static void printDI(int n){
        printD(n);
        printI(n);
    }
    public static void main(String[] args) {
        int n = 5;
        printDI(n);
    }
}
